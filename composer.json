{
    "name": "techinasia/linkedin-pdf-resume-parser",
    "type": "library",
    "description": "Parse LinkedIn PDF Resume and extract out name, email, education and work experiences.",
    "keywords": [
        "linkedin",
        "pdf",
        "resume",
        "parser",
        "techinasia"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Tech in Asia Developers",
            "email": "dev@techinasia.com"
        }
    ],
    "require": {
        "smalot/pdfparser": "*",
        "roave/security-advisories": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "mockery/mockery": "^0.9.8"
    },
    "autoload": {
        "psr-4": {
            "LinkedInResumeParser\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LinkedInResumeParser\\Tests\\": "tests/"
        }
    }
}
